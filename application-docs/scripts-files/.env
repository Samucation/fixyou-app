SPRINT_SECURITY_BASIC=false

#FIXOYOU INTERNAL CONFIGURATION IN WEB APPLICATION KEYCLOAK IN MODULE REGISTRATIONS
KEYCLOAK_ISSUER_URI=http://<local-ip>:8080/realms/fixyourealm
KEYCLOAK_JWK_SET_URI=http://<local-ip>:8080/realms/fixyourealm/protocol/openid-connect/certs
CLIENT_VALUE=fixyou-client-ext

#CORS CONFIGURATION IN MODULE REGISTRATION
CORS_ALLOWED_LIST=http://localhost:8081,http://localhost:8082,http://localhost:4200

#OUTHERS CONFIGURATION IN MODULE REGISTRATIONS
SECURITY_LOG_LEVEL=DEBUG
WEB_LOG_LEVEL=DEBUG

#MIGRATIONS CONFIGURATIONS IN MODULE REGISTRATIONS
FLYWAY_CLEAN_DISABLED=true
FLYWAY_DEFAULT_SCHEMA=fixyou
FLYWAY_SCHEMAS=fixyou

# - GERAL DOCKER IMAGE FIXYOU -

#APPLICATION EXTERNAL PORT IN MODULE DOCKER
DOCKER_EXTERNAL_APP_PORT=8082
API_FIXYOU_CONTAINER_NAME=api-fixyou

#APPLICATION NETWORK OF MODULES IN DOCKER
DOCKER_NETWORK=fixyou-network

#POSTGRES - MODULE REGISTRATIONS IN DOCKER
DOCKER_POSTGRES_CONTAINER_NAME=fixyou-postgres
DOCKER_POSTGRES_DB_NAME=m33_db
