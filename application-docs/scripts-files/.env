#APPLICATION FIXYOU MODULE - REGISTRATIONS
#AUTENTICATION DB IN MODULE REGISTRATIONS
JDBC_DATABASE_URL=jdbc:postgresql://postgres:5432/m33_db
JDBC_DATABASE_USERNAME=postgres
JDBC_DATABASE_PASSWORD=F7@uM!dQ#2kLpZxT$9eV

#CONFIGURATION DB IN MODULE REGISTATIONS
HIBERNATE_DDL_AUTO=update
JPA_SHOW_SQL=true
HIBERNATE_DIALECT=org.hibernate.dialect.PostgreSQLDialect
SQL_LOG_LEVEL=DEBUG
SQL_BIND_LOG_LEVEL=TRACE

#SERVER PORT IN MODULE APPLICATION FIXYOU MODULE REGISTRATIONS
