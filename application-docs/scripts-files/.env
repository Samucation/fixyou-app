#KEYCLOAK CONFIGURATIONS - MODULE REGISTRATIONS IN DOCKER
KEYCLOAK_IMAGE=samucation/keycloak:latest
KEYCLOAK_CONTAINER_NAME=keycloak
KEYCLOAK_HOST_NAME=<local-ip>
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASS=admin
KEYCLOAK_SCHEMA=postgres
KEYCLOAK_DB_URL=jdbc:postgresql://postgres:${DOCKER_POSTGRES_EXTERNAL_PORT}/${DOCKER_POSTGRES_DB_NAME}
KEYCLOAK_DB_USERNAME=postgres
KEYCLOAK_RESTRICT=false
KEYCLOAK_DOCKER_EXTERNAL_PORT=8080

#FLYWAY - MODULE REGISTRATIONS IN DOCKER
DOCKER_FLYWAY_CONTAINER_NAME=flyway-fixyou
DOCKER_FLYWAY_SCHEMA=fixyou
